<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zkadisa.personalmoviedb">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/pmdb_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pmdb_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".L1.FirstActivity"></activity>
        <activity android:name=".L1.SecondActivity"></activity>
        <activity android:name=".L1.ThirdActivity"></activity>
        <activity android:name=".L1.FourthActivity"></activity>
        <activity android:name=".L1.DetailsActivity"></activity>
        <activity android:name=".DetailsActivity"></activity>
    </application>

</manifest>